.log-list {
  display: flex;
  flex-direction: column;
}
.dlist{
  width: 100%;
  margin-top: 25rpx;
}
.dlist dl{
  width: 100%;
  display: flex;
  margin-top: 50rpx;
}
.dlist dl dt{
  margin-left: 35rpx;
}
.dlist image{
  width: 400rpx;
  height: 200rpx;
}
.dlist dl dd{
  margin-left: 25rpx;
}
.dlist dl dd p{
  display: inline-block;
  margin: 10rpx 0;

}
.dlist dl dd p:nth-child(1){
  font-size: 36rpx;
}

.dlist dl dd p:nth-child(2),.dlist dl dd p:nth-child(3){
  font-size: 32rpx;
  color: #666;
}

